START OF LITERARY ARTISITIC 

 <flow:Map rdf:ID="n0o27">
    <flow:sparql><![CDATA[prefix foaf: <http://xmlns.com/foaf/0.1/> prefix bibo: <http://purl.org/ontology/bibo/> ASK {  { ?a a bibo:Book } UNION { ?a a bibo:Document }. }]]></flow:sparql>
    <flow:assumption>"Assuming that in the database of Bibliographica a literary/artistic work necessarily qualifies as either bibo:Book or bibo:Document"</flow:assumption>
    <rdfs:domain rdf:resource="#n0" />
    <rdfs:range rdf:resource="#o27" />
  </flow:Map>


<flow:Map rdf:ID="n46o49">
	<flow:sparql><![CDATA[ prefix xsd: <http://www.w3.org/2001/XMLSchema#> prefix skos: <http://www.w3.org/2004/02/skos/core#> prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>  prefix bibo: <http://purl.org/ontology/bibo/> prefix foaf: <http://xmlns.com/foaf/0.1/> ASK { { ?x a skos:Concept ;  skos:notation ?y . FILTER( xsd:float(?y) >= 340 && xsd:float(?y) < 350) }  } ]]></flow:sparql>
	<flow:assumption>"Assuming that the work of a text of a treaty, law or regulation or an official translation of such a text qualifies as a legal document as classified by dewey notation (is within the range of 340 - 349) Note: this dewey range contains documents which are not legal or composed by legal entities"</flow:assumption>
	<rdfs:domain rdf:resource="#n46" />
	<rdfs:range rdf:resource="#o49" />
</flow:Map>

<flow:Map rdf:ID="n46o50">
	<flow:sparql-not><![CDATA[ASK {}]]></flow:sparql-not>
	<flow:assumption>"Assuming that A report or decision of an administrative, judicial body or of any other body or State authority or the administration or an official translation of such a text will qualify in the dewey range for legal works "</flow:assumption>
	<rdfs:domain rdf:resource="#n46" />
	<rdfs:range rdf:resource="#o50" />
</flow:Map>

<flow:Map rdf:ID="n46o48">
	<flow:sparql-not><![CDATA[ prefix xsd: <http://www.w3.org/2001/XMLSchema#> prefix skos: <http://www.w3.org/2004/02/skos/core#> prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>  prefix bibo: <http://purl.org/ontology/bibo/> prefix foaf: <http://xmlns.com/foaf/0.1/> ASK { { ?x a skos:Concept ;  skos:notation ?y . FILTER( xsd:float(?y) >= 340 && xsd:float(?y) < 350) } } ]]></flow:sparql-not>
	<flow:assumption>"Assuming that the work not  falling under dewey classification of legal works would neccesarily qualify as other"</flow:assumption>
	<rdfs:domain rdf:resource="#n46" />
	<rdfs:range rdf:resource="#o48" />
